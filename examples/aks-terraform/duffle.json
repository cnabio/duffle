{
    "name": "aks-terraform",
    "version": "0.1.0",
    "schemaVersion": "v1.0.0",
    "invocationImages": {
        "cnab": {
            "name": "cnab",
            "builder": "docker",
            "configuration": {
                "registry": "cnab"
            }
        }
    },
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "East US",
            "destination": {
                "env": "TF_VAR_location"
            }
        },
        "kubernetes_version": {
            "type": "string",
            "defaultValue": "1.12.5",
            "destination": {
                "env": "TF_VAR_kubernetes_version"
            }
        },
        "agent_count": {
            "type": "integer",
            "defaultValue": 1,
            "destination": {
                "env": "TF_VAR_agent_count"
            }
        },
        "dns_prefix": {
            "type": "string",
            "defaultValue": "akstest",
            "destination": {
                "env": "TF_VAR_dns_prefix"
            }
        },
        "cluster_name": {
            "type": "string",
            "defaultValue": "akstest",
            "destination": {
                "env": "TF_VAR_cluster_name"
            }
        },
        "resource_group_name": {
            "type": "string",
            "defaultValue": "azure-akstest",
            "destination": {
                "env": "TF_VAR_resource_group_name"
            }
        },
        "backend": {
            "type": "boolean",
            "defaultValue": true,
            "destination": {
                "env": "TF_VAR_backend"
            }
        },
        "backend_storage_account": {
            "type": "string",
            "defaultValue": "aksterraformduffle",
            "destination": {
                "env": "TF_VAR_backend_storage_account"
            }
        },
        "backend_storage_container": {
            "type": "string",
            "defaultValue": "aksterraformduffle",
            "destination": {
                "env": "TF_VAR_backend_storage_container"
            }
        },
        "backend_storage_resource_group": {
            "type": "string",
            "defaultValue": "aksterraformduffle",
            "destination": {
                "env": "TF_VAR_backend_storage_resource_group"
            }
        }
    },
    "credentials": {
        "tenant_id": {
            "env": "TF_VAR_tenant_id"
        },
        "client_id": {
            "env": "TF_VAR_client_id"
        },
        "client_secret": {
            "env": "TF_VAR_client_secret"
        },
        "subscription_id": {
            "env": "TF_VAR_subscription_id"
        },
        "ssh_authorized_key": {
            "env": "TF_VAR_ssh_authorized_key"
        }
    }
}

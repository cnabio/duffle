        },
        "nats-streaming": {
            "imageType": "docker",
            "image": "nats-streaming:0.11.2",
            "digest": "sha256:d70706ce2e92efa6daa193731cd3aec98aedf3c15c51c2cf5b7a6bbc80a43044"
        },
       "queue-worker": {
            "imageType": "docker",
            "image": "openfaas/queue-worker:0.5.4",

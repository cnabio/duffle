    "maintainers": [
        {
            "name": "John McCabe",
            "email": "john@johnmccabe.net",
            "url": "https://github.com/johnmccabe"
        }
    ],
    "parameters": {
        "basic_auth": {
            "type": "string",
            "defaultValue": "true",
            "destination": {
                "env": "BASIC_AUTH"
            }
        }
